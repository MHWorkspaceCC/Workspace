              "protocol": "TCP",
              "sourcePortRange": "*",
              "destinationPortRange": "3389",
              "sourceAddressPrefix": "[variables('jumpCIDR')]",
              "destinationAddressPrefix": "*",
              "access": "Allow",
              "priority": 1002,
