{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnetCidrPrefix": {
      "type": "string"
    },
    "resourceNamePostfix": {
      "type": "string"
    },
    "vnetName": {
      "type": "string"
    },
    "environment": {
      "type": "string"
    },
    "environmentCode": {
      "type": "string"
    },
    "subscriptionCode": {
      "type": "string"
    },
    "facility": {
      "type": "string"
    },
    "instance": {
      "type": "string"
    },
    "role": {
      "type": "string"
    },
    "location": {
      "type": "string"
    }
  },

  "variables": {
    "baseName": "[concat('-', parameters('resourceNamePostfix'))]",
    "vnetName": "[concat(parameters('vnetName'), '-vnet', variables('baseName'))]",

    "addressPrefix": "[parameters('vnetCidrPrefix')]",
    "vnetCIDR": "[concat(variables('addressPrefix'), '0.0/16')]",

    "defaultCIDR": "[concat(variables('addressPrefix'), '0.0/24')]",
    "webCIDR": "[concat(variables('addressPrefix'), '1.0/24')]",
    "ftpCIDR": "[concat(variables('addressPrefix'), '2.0/24')]",
    "jumpCIDR": "[concat(variables('addressPrefix'), '3.0/24')]",
    "adminCIDR": "[concat(variables('addressPrefix'), '4.0/24')]",
    "dbCIDR": "[concat(variables('addressPrefix'), '5.0/24')]",
    "vpnCIDR": "[concat(variables('addressPrefix'), '100.0/24')]",

    "defaultSnName": "default",
    "webSnName": "[concat('sn-web', variables('baseName'))]",
    "ftpSnName": "[concat('sn-ftp', variables('baseName'))]",
    "jumpSnName": "[concat('sn-jump', variables('baseName'))]",
    "adminSnName": "[concat('sn-admin', variables('baseName'))]",
    "dbSnName": "[concat('sn-db', variables('baseName'))]",
    "vpnSnName": "[concat('sn-vpn', variables('baseName'))]",

    "dbNsgName": "[concat('nsg-db', variables('baseName'))]",
    "webNsgName": "[concat('nsg-web', variables('baseName'))]",
    "ftpNsgName": "[concat('nsg-ftp', variables('baseName'))]",
    "jumpNsgName": "[concat('nsg-jump', variables('baseName'))]",
    "adminNsgName": "[concat('nsg-admin', variables('baseName'))]",
    "vpnNsgName": "[concat('nsg-vpn', variables('baseName'))]",
    "defaultNsgName": "[concat('nsg-default', variables('baseName'))]",

    "nsgRg": "[concat('rg-nsgs', variables('baseName'))]"
  },

  "resources": [

    {
      "type": "Microsoft.Network/virtualNetworks",
      "name": "[variables('vnetName')]",
      "apiVersion": "2017-06-01",
      "location": "[resourceGroup().location]",
      "scale": null,
      "tags": {
        "Role": "[parameters('role')]",
        "ResourceType": "VNET",
        "Environment": "[parameters('environment')]",
        "EnvironmentCode": "[parameters('environmentCode')]",
        "Facility": "[parameters('facility')]",
        "Location": "[parameters('location')]",
        "Instance": "[parameters('instance')]",
        "ResourceNamePostfix": "[parameters('resourceNamePostfix')]",
        "SubscriptionCode": "[parameters('subscriptionCode')]"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('vnetCIDR')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('defaultSnName')]",
            "properties": {
              "addressPrefix": "[variables('defaultCIDR')]",
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('defaultNsgName'))]"
              }
            }
          },
          {
            "name": "[variables('webSnName')]",
            "properties": {
              "addressPrefix": "[variables('webCIDR')]",
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('webNsgName'))]"
              }
            }
          },
          {
            "name": "[variables('ftpSnName')]",
            "properties": {
              "addressPrefix": "[variables('ftpCIDR')]",
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('ftpNsgName'))]"
              }
            }
          },
          {
            "name": "[variables('jumpSnName')]",
            "properties": {
              "addressPrefix": "[variables('jumpCIDR')]",
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('jumpNsgName'))]"
              }
            }
          },
          {
            "name": "[variables('adminSnName')]",
            "properties": {
              "addressPrefix": "[variables('adminCIDR')]",
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('adminNsgName'))]"
              }
            }
          },
          {
            "name": "[variables('dbSnName')]",
            "properties": {
              "addressPrefix": "[variables('dbCIDR')]",
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('dbNsgName'))]"
              }
            }
          },
          {
            "name": "GatewaySubnet",
            "properties": {
              "addressPrefix": "[variables('vpnCIDR')]"
            }
          }

        ],
        "virtualNetworkPeerings": []
      },
      "dependsOn": [
        "[resourceId('Microsoft.Network/networkSecurityGroups', variables('defaultNsgName'))]",
        "[resourceId('Microsoft.Network/networkSecurityGroups', variables('dbNsgName'))]",
        "[resourceId('Microsoft.Network/networkSecurityGroups', variables('webNsgName'))]",
        "[resourceId('Microsoft.Network/networkSecurityGroups', variables('ftpNsgName'))]",
        "[resourceId('Microsoft.Network/networkSecurityGroups', variables('jumpNsgName'))]",
        "[resourceId('Microsoft.Network/networkSecurityGroups', variables('adminNsgName'))]"
